    <string name="pref_default_file_manager_path_title">Ruta predeterminada del administrador de archivos</string>

    <string name="pref_default_audio_language_title">Idiomas predeterminados de audio</string>
    <string name="pref_default_audio_language_message">Seleccione los idiomas de audio que se seleccionarán de manera predeterminada al reproducir un vídeo con varias transmisiones de audio.\nLos códigos de idiomas de dos o tres letras suelen funcionar. Los valores múltiples se pueden delimitar con comas.</string>

    <string name="pref_default_subtitle_language_title">Idiomas predeterminados de subtítulos</string>
    <string name="pref_default_subtitle_language_message">Seleccione los idiomas de los subtítulos que se seleccionarán de manera predeterminada al reproducir un vídeo con varios subtítulos.\nLos códigos de idiomas de dos o tres letras suelen funcionar. Los valores múltiples se pueden delimitar con comas.</string>

    <string name="pref_hardware_decoding_title">Descodificación por hardware</string>
    <string name="pref_hardware_decoding_summary">Intentar la descodificación por hardware antes de volver a la de software</string>

    <string name="pref_background_play_title">Reproducción en segundo plano</string>
    <string name="pref_background_play_summary">Seleccione cuándo se debe reanudar automáticamente la reproducción en segundo plano</string>

    <string name="pref_save_position_title">Guardar posición al salir</string>
    <string name="pref_save_position_summary">Recuerda la posición de reproducción actual al salir. Cuando el mismo archivo se reproduce de nuevo, mpv se desplazará a la posición anterior.</string>

    <string name="pref_auto_rotation_title">Orientación de pantalla</string>
    <string name="pref_auto_rotation_summary">Decida en qué orientación mpv reproducirá vídeos horizontales o verticales</string>

    <string name="pref_display_media_title_title">Mostrar título de multimedia (vídeos)</string>
    <string name="pref_display_media_title_summary">Muestra el título o el nombre de archivo de los vídeos encima de los controles. Los archivos de audio siempre mostrarán sus metadatos.</string>

    <string name="pref_bottom_controls_title">Mostrar controles en parte inferior de pantalla</string>

    <string name="pref_no_ui_pause_title">Continuar reproducción durante ventanas emergentes de interfaz de usuario</string>
    <string name="pref_no_ui_pause_summary">Seleccione si la reproducción debe continuar mientras las ventanas emergentes están abiertas (por ejemplo, el cuadro de diálogo de la lista de reproducción)</string>

    <string name="pref_seek_gesture_smooth_title">Desplazamiento más suave</string>

    <string name="pref_gesture_horiz_title">Arrastre horizontal</string>

    <string name="pref_gesture_vert_left_title">Arrastre vertical (lado izquierdo)</string>

    <string name="pref_gesture_vert_right_title">Arrastre vertical (lado derecho)</string>

    <string name="pref_gesture_tap_left_title">Doble toque (izquierda)</string>

    <string name="pref_gesture_tap_center_title">Doble toque (centro)</string>

    <string name="pref_gesture_tap_right_title">Doble toque (derecha)</string>

    <string name="pref_gesture_custom_helptext">Cuando un gesto de toque está configurado como \"Personalizado\", se puede vincular a cualquier comando editando input.conf. Los códigos clave son <b>0x10001</b> (izquierda), <b>0x10002</b> (centro), <b>0x10003</b> (derecha).\nPor ejemplo, puedes vincular para desplazarse por 6s:\n\t\t0x10003 no-osd seek 6</string>

    <string name="pref_video_upscale_title">Filtro de aumento de escala</string>

    <string name="pref_video_downscale_title">Filtro de reducción de escala</string>

    <string name="pref_video_debanding_title">Desbandeado</string>
    <string name="pref_video_debanding_summary">Seleccione el modo de desbandeado de vídeo</string>

    <string name="pref_video_interpolation_title">Interpolación</string>
    <string name="pref_video_interpolation_message">Reduce las vibraciones causadas por el desajuste entre los FPS de vídeo y la frecuencia de actualización de la pantalla.</string>

    <string name="pref_video_tscale_title">Filtro de interpolación temporal</string>
    <string name="pref_video_tscale_summary">Seleccione el filtro utilizado para interpolar el eje temporal (fotogramas)</string>
    <string name="pref_video_tscale_message">Esta configuración solo tiene efecto si la interpolación está activada arriba.</string>

    <string name="pref_video_fastdecode_title">Descodificación de vídeo de baja calidad</string>
    <string name="pref_video_fastdecode_summary">Cambia la calidad por el rendimiento y, por lo tanto, una reproducción fluida.\n<b>REDUCE MUCHO LA CALIDAD</b></string>

    <string name="pref_ignore_audio_focus_title">Ignorar enfoque de audio</string>
    <string name="pref_ignore_audio_focus_summary">No pausa la reproducción ni reduce el volumen cuando otras aplicaciones reproducen audio al mismo tiempo.</string>

    <string name="pref_stats_mode_title">Mostrar estadísticas</string>
    <string name="pref_stats_mode_summary">Seleccione qué estadísticas se muestran</string>

