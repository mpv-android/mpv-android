    <string name="pref_default_audio_language_title">Domyślny język dźwięku</string>
    <string name="pref_default_audio_language_message">Język(i) dźwięku do domyślnego wybierania podczas odtwarzania wideo z wieloma strumieniami audio.\nDziałają dwu- lub trzyliterowe kody języków. Wiele wartości można oddzielić przecinkami.</string>

    <string name="pref_default_subtitle_language_title">Domyślny język napisów</string>
    <string name="pref_default_subtitle_language_message">Język(i) napisów do domyślnego wybierania podczas odtwarzania wideo z wieloma napisami.\nDziałają dwu- lub trzyliterowe kody języków. Wiele wartości można oddzielić przecinkami.</string>

    <string name="pref_hardware_decoding_title">Dekodowanie sprzętowe</string>
    <string name="pref_hardware_decoding_summary">Spróbuj dekodowania sprzętowego, zanim powrócisz do programowego. Jest to na ogół bardziej wydajne.</string>

    <string name="pref_background_play_title">Odtwarzanie w tle</string>
    <string name="pref_background_play_summary">Decyduj, kiedy odtwarzanie w tle ma zostać automatycznie wznowione</string>

    <string name="pref_save_position_title">Zapisz pozycję po wyjściu</string>
    <string name="pref_save_position_summary">Zapamiętaj bieżącą pozycję odtwarzania przy wyjściu. Kiedy ten sam plik zostanie odtworzony ponownie, mpv przejdzie do poprzedniej pozycji.</string>

    <string name="pref_auto_rotation_title">Orientacja ekranu</string>
    <string name="pref_auto_rotation_summary">Zdecyduj, w jakiej orientacji mpv będą odtwarzane filmy w orientacji poziomej czy pionowej.</string>

    <string name="pref_display_media_title_title">Wyświetl tytuł multimediów (wideo)</string>
    <string name="pref_display_media_title_summary">Wyświetlaj tytuł lub nazwę pliku wideo nad elementami sterującymi. Pliki dźwiękowe zawsze będą wyświetlać swoje metadane.</string>

    <string name="pref_bottom_controls_title">Pokaż elementy sterujące na dole ekranu</string>

    <string name="pref_no_ui_pause_title">Kontynuuj odtwarzanie przy wyskakujących okienkach interfejsu użytkownika</string>
    <string name="pref_no_ui_pause_summary">Kontynuuj odtwarzanie, gdy wyskakujące okienka są otwarte (np. okno dialogowe listy odtwarzania).</string>

    <string name="pref_playlist_exit_warning_title">Potwierdzenie opuszczenia listy odtwarzania</string>
    <string name="pref_playlist_exit_warning_summary">Wyświetlaj okno dialogowe potwierdzenia przed wyjściem, gdy załadowana jest lista odtwarzania.</string>

    <string name="pref_seek_gesture_smooth_title">Płynniejsze przewijanie</string>

    <string name="pref_gesture_horiz_title">Przeciąganie w poziomie</string>

    <string name="pref_gesture_vert_left_title">Przeciąganie w pionie (lewa strona)</string>

    <string name="pref_gesture_vert_right_title">Przeciąganie w pionie (prawa strona)</string>

    <string name="pref_gesture_tap_left_title">Podwójne stuknięcie (po lewej)</string>

    <string name="pref_gesture_tap_center_title">Podwójne stuknięcie (na środku)</string>

    <string name="pref_gesture_tap_right_title">Podwójne stuknięcie (po prawej)</string>

    <string name="pref_gesture_custom_helptext">Gdy gest dotknięcia jest ustawiony na „Niestandardowy”, można go powiązać z dowolnym poleceniem, edytując plik input.conf. Kody klawiszy to <b>0x10001</b> (po lewej), <b>0x10002</b> (w środku), <b>0x10003</b> (po prawej).\nMożna na przykład powiązać przewijanie po 6 sekundach:\n\t\t0x10003 no-osd seek 6</string>

    <string name="pref_video_upscale_title">Filtr zwiększający skalę</string>

    <string name="pref_video_downscale_title">Filtr zmniejszający skalę</string>

    <string name="pref_video_debanding_title">Usuwanie pasm</string>
    <string name="pref_video_debanding_summary">Wybierz tryb usuwania pasm wideo.</string>

    <string name="pref_video_interpolation_title">Interpolacja</string>
    <string name="pref_video_interpolation_message">Zmniejsz drgania spowodowane niedopasowaną liczbą klatek na sekundę wideo i częstotliwością odświeżania wyświetlacza.</string>

    <string name="pref_video_tscale_title">Filtr interpolacji czasowej</string>
    <string name="pref_video_tscale_summary">Wybierz filtr używany do interpolacji osi czasowej (klatek).</string>
    <string name="pref_video_tscale_message">Te ustawienia obowiązują tylko wtedy, gdy powyżej włączona jest interpolacja.</string>

    <string name="pref_video_fastdecode_title">Dekodowanie wideo niskiej jakości</string>
    <string name="pref_video_fastdecode_summary">Zamienia jakość na wydajność, a tym samym płynne odtwarzanie.\n<b>ZNACZNIE OBNIŻA JAKOŚĆ</b></string>

    <string name="pref_ignore_audio_focus_title">Ignoruj fokus dźwięku</string>
    <string name="pref_ignore_audio_focus_summary">Nie wstrzymuj odtwarzania ani nie zmniejszaj głośności, gdy inne aplikacje odtwarzają dźwięk w tym samym czasie.</string>

    <string name="pref_stats_mode_title">Pokaż statystyki</string>
    <string name="pref_stats_mode_summary">Wybierz, które statystyki mają być wyświetlane</string>

    <string name="pref_gpu_debug_title">Włącz debugowanie OpenGL</string>

    <string name="pref_gpu_next_title">Używaj gpu-next</string>
    <string name="pref_gpu_next_summary">Używaj zupełnie nowego backendu do renderowania wideo, opartego na libplacebo.</string>

