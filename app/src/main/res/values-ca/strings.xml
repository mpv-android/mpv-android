    <string name="pref_save_position_title">Desa la posició en sortir</string>
    <string name="pref_save_position_summary">Recorda la posició de reproducció actual en sortir. Quan es torni a reproduir el mateix fitxer mpv anirà a la posició desada.</string>

    <string name="pref_auto_rotation_title">Orientació de pantalla</string>
    <string name="pref_auto_rotation_summary">Decideix l'orientació, horitzontal o vertical, en què mpv reproduirà els vídeos.</string>
    
    <string name="pref_display_media_title_title">Mostra el títol (del vídeo)</string>
