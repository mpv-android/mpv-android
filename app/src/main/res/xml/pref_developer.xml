<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	
	<ListPreference
			android:defaultValue=""
			android:key="stats_mode"
			android:summary="@string/pref_developer_stats_summary"
			android:entries="@array/stats_entries"
			android:entryValues="@array/stats_values"
			android:title="@string/pref_developer_stats_title"/>
	
	<CheckBoxPreference
			android:defaultValue="false"
			android:key="ignore_audio_focus"
			android:summary="@string/pref_developer_ignore_audio_focus_summary"
			android:title="@string/pref_developer_ignore_audio_focus_title"/>
	
	<CheckBoxPreference
			android:defaultValue="false"
			android:key="gpudebug"
			android:title="@string/pref_developer_gpudebug_title"/>

</PreferenceScreen>
